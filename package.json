{
  "name": "react-golang",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "tailwind": "npx tailwindcss build -i ./src/frontend/index.css -o ./src/frontend/tailwind.css --watch",
    "backend": "nodemon --ext go --exec \"node dev.cjs || exit 1\"",
    "dev": "concurrently \"npm run tailwind\" \"npm run backend\" \"vite\"",
    "build-frontend": "tsc && vite build",
    "build-backend": "node build.cjs",
    "build": "npm run build-frontend && npm run build-backend",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "node start.cjs"
  },
  "dependencies": {
    "@nextui-org/react": "^2.4.1",
    "@tanstack/react-query": "^5.40.1",
    "axios": "^1.7.2",
    "classnames": "^2.5.1",
    "concurrently": "^8.2.2",
    "formik": "^2.4.6",
    "framer-motion": "^11.2.10",
    "next-themes": "^0.3.0",
    "nodemon": "^3.1.2",
    "react": "^18.2.0",
    "react-ace": "^11.0.1",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1",
    "react-rnd": "^10.4.11",
    "react-router-dom": "^6.23.1",
    "react-toastify": "^10.0.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
